package com.moviestream.movie.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.moviestream.movie.domain.BoardDTO;
import com.moviestream.movie.persistence.IBoardDAO;
import com.moviestream.movie.service.IBoardService;

@Service
public class BoardServiceImpl implements IBoardService{
	
	@Autowired
	private IBoardDAO bDao;

	@Override
	public List<BoardDTO> boardList() throws Exception {
		
		return bDao.boardList();
	}

}
