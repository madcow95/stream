package com.moviestream.movie.persistence.impl;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;

import com.moviestream.movie.domain.MemberDTO;
import com.moviestream.movie.persistence.IMemberDAO;

public class MemberDAOImpl implements IMemberDAO{
	
	@Autowired SqlSession session;

	@Override
	public MemberDTO getMember() throws Exception {

		return session.selectOne("MemberMapper.getMember");
	}

}
